{
  "name": "@casoon/ui-lib",
  "version": "0.3.11",
  "description": "Ein schlankes Design-System für CASOON-Projekte - Optimiert für moderne Webentwicklung mit Astro JS und LightningCSS",
  "main": "core.css",
  "style": "core.css",
  "files": [
    "layers/",
    "themes/",
    "icons/",
    "modules/",
    "effects/",
    "*.css",
    "LICENSE",
    "README.md",
    "LAYER-SYSTEM.md",
    "STYLE-GUIDE.md"
  ],
  "scripts": {
    "lint": "stylelint \"**/*.css\" --ignore-path .stylelintignore",
    "lint:fix": "stylelint \"**/*.css\" --fix --ignore-path .stylelintignore",
    "test": "npm run lint",
    "prepublishOnly": "npm test",
    "icons:build": "node scripts/build-icons.js",
    "bump-version": "node scripts/bump-version.js",
    "test:lightningcss": "cd tests/css-processing && npm run test",
    "prerelease": "npm run test:lightningcss && npm run lint"
  },
  "keywords": [
    "css",
    "framework",
    "ui",
    "design-system",
    "accessibility",
    "components",
    "responsive",
    "modern",
    "icons",
    "astro",
    "lightningcss",
    "ssr",
    "css-layers"
  ],
  "author": "CASOON",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casoon/casoon-ui-lib.git"
  },
  "bugs": {
    "url": "https://github.com/casoon/casoon-ui-lib/issues"
  },
  "homepage": "https://casoon.github.io/casoon-ui-docs/",
  "devDependencies": {
    "@iconify-json/bi": "^1.2.2",
    "@iconify-json/feather": "^1.2.1",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/ph": "1.2.2",
    "@iconify-json/ri": "1.2.5",
    "@iconify/json": "^2.2.328",
    "fs-extra": "11.3.0",
    "stylelint": "^16.18.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-declaration-strict-value": "1.10.11",
    "stylelint-use-logical": "^2.1.2"
  }
}
